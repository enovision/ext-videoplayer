{
  "name": "ext-videoplayer",
  "sencha": {
    "namespace": "VideoPlayer",
    "type": "code",
    // "toolkit": "classic",
    "creator": "J.J. van de Merwe",
    "summary": "Videoplayer based on Video.js",
    "detailedDescription": "Videoplayer based on Video.js",
    "version": "1.0.0",
    "compatVersion": "1.0.0",
    "format": "1",
    "slicer": {
      "js": [
        {
          "path": "${package.dir}/sass/example/custom.js",
          "isWidgetManifest": true
        }
      ]
    },
    "output": "${package.dir}/build",
    "local": true,
    "sass": {
      "namespace": "VideoPlayer",
      "etc": [
        "${package.dir}/sass/etc/all.scss"
      ],
      "var": [
        "${package.dir}/sass/var"
      ],
      "src": [
        "${package.dir}/sass/src"
      ]
    },
    "classpath": [
      "${package.dir}/src"
    ],
    "resources": [
      {
        "path": "resources",
        "output": "shared"
      }
    ],
    "requires": []
  }
}
